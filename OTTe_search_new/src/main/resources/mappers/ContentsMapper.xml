<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.zerock.mapper.ContentsMapper">

	<select id="read" resultType="org.zerock.domain.ContentsVO">
		select
			CONTENTSID,NATIONID,OTTID,GRADEID,CLASSID,CONTENTSTITLE,CONTENTSSUMMARY,
			CONTENTSYEARS,CONTENTSSEASON,CONTENTSEPISODE,CONTENTSSCORE,CONTENTSDURATION
		from
			contents_t;
		where CONTENTSID = #{CONTENTSID}
	</select>
</mapper>
